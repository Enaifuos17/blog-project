<div class="author">
  <h1 class="author-heading">Search Results</h1>
</div>
<section class="articles">
  <ul class="article-ul">
    <!-- loop 3la data -->
    <% data.forEach(post => { %>

    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="article-list-date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>

    <% }) %>
  </ul>
</section>
